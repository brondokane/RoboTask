<!DOCTYPE html>
<html>
<head>
  <title>ProjectProposalMaster</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user %>
  logged in as: <%= current_user.email %>
  <%= button_to "Logout", session_url, method: :delete %>
<% else %>
  <%= link_to "Signup", new_user_url %>
  <%= link_to "Login", new_session_url %>
<% end %>

<%= flash[:errors].each { |error| error } if flash[:errors] %>
<%= flash[:notices].each { |notice| notice } if flash[:notices] %>


<%= yield %>

</body>
</html>
